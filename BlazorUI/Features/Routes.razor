<Router AppAssembly="@typeof(App).Assembly" NotFoundPage="typeof(Features.Home.Pages.NotFound)">
    <Found Context="routeData">
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
        <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)">
            <NotAuthorized>
                <NotAuthorized />
            </NotAuthorized>
        </AuthorizeRouteView>
    </Found>
</Router>
